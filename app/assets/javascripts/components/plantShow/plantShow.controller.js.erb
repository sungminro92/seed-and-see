angular
  .module('seedAndSee')
  .controller('PlantShowController', PlantShowController);

PlantShowController.$inject = ['$stateParams','plantsService'];

function PlantShowController($stateParams, plantsService) {
  var vm = this;

  vm.plant = {};

  // var id = $stateParams.id;
    plantsService
      .plantShow($stateParams.id)
      .then(function(res) {
        console.log(res);
        vm.plant = res.data;
      });

}